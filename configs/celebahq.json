{
    "save_dir": "release_model/",
    "seed": 2020,
    "data_loader": {
        "name": "celebahq", 
        "zip_root": "../datazip",
        "flist_root": "../flist",
        "mask": "square",
        "w": 512,
        "h": 512
    },
    "losses": {
        "spatial_discounting_gamma": 0.9,
        "coarse_l1_alpha": 1.2,
        "l1_loss_alpha": 1.2,
        "ae_loss_alpha": 1.2,
        "gan_loss_alpha": 0.001,
        "global_wgan_loss_alpha": 1,
        "wgan_gp_lambda": 10
    },
    "trainer": {
        "type": "Adam",
        "lr": 1e-4,
        "d2glr": 1, 
        "beta1": 0.5,
        "beta2": 0.9,
        "verbosity": 2,
        "iterations": 33e4,
        "save_freq": 1e4,
        "valid_freq": 1e4,
        "niter": 30e4,
        "niter_steady": 1e4,
        "batch_size": 16,
        "num_workers": 4,
        "log_step": 100,
        "n_critic": 5
    }
}
