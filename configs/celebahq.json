{
    "save_dir": "release_model/",
    "seed": 2020,
    "netG": {
        "input_dim": 3,
        "ngf": 32
    },
    "netD": {
        "input_dim": 3,
        "ndf": 64
    },
    "data_loader": {
        "name": "celebahq", 
        "zip_root": "../datazip",
        "flist_root": "../flist",
        "w": 512,
        "h": 512,
        "mask_shape": 128,
        "max_delta_shape": 32,
        "margin": 0,
        "batch_size": 16,
        "num_workers": 4
    },
    "losses": {
        "spatial_discounting_gamma": 0.9,
        "coarse_l1_alpha": 1.2,
        "l1_loss_alpha": 1.2,
        "ae_loss_alpha": 1.2,
        "gan_loss_alpha": 0.001,
        "global_wgan_loss_alpha": 1,
        "wgan_gp_lambda": 10,
        "ae_loss_alpha": 1.2
    },
    "metrics": [
        "mae",
        "psnr",
        "ssim"
    ],
    "optimizer": {
        "type": "Adam",
        "lr": 0.0001,
        "beta1": 0.5,
        "beta2": 0.9,
        "weight_decay": 0,
        "amsgrad": true
    },
    "trainer": {
        "iterations": 200000,
        "save_freq": 5000,
        "valid_freq": 10000,
        "verbosity": 2,
        "mask_only": true,
        "n_critic": 5
    },
    "logger": {
        "log_step": 100,
        "tensorboardX": true
    }
}
